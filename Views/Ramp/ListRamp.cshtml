@model IEnumerable<wrts.Models.Ramp>

@{
    ViewData["Title"] = "ListRamp";
}

<h1>ListRamp</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VehiclesID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VehiclesID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RampID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RampID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RampID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
